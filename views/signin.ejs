<html>
    <head>
        <% include partials/head.ejs %>
        <% include partials/navbar.ejs %>
    </head>
    <body>
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <h1>SignUp Page</h1>
               
                
               
                <form action="/users/signin" method="post">
                    <div class="form-group">
                        <label for="email">E-Mail</label>
                        <input type="text" class="form-control" id="email" name="email" placeholder="e@mail.com">
                    </div>

                    <div class="form-group">
                        <label for="password">Password</label>
                        <input type="password" class="form-control" id="password" name="password" placeholder="ex that you miss">
                    </div>

                    <button class="btn btn-default btn-info" type="submit">Sign In</button>
                </form>
            </div>
        </div>
        <% include partials/footer.ejs %>
        <% include partials/scripts.ejs %>
    </body>
</html>